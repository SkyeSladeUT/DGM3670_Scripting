//xform -boundingBox
//midpoint formula
//(x1+x2)/2, (y1+y2)/2, (z1+z2)/2
{
	proc vector GetCenterOfAll(){
		string $objects = `ls -sl`;
		vector $position;
		int $xPos = 0, $yPos = 0, $zPos = 0;
		for($obj in $objects){
			$position = `xform -q -translation`;
			$xPos += $position.x;
			$yPos += $position.y;
			$zPos += $position.z;
		}
	}

}